{
  "manifest_version": 3,
  "name": "Tab Transfer Tool",
  "version": "1.0.0",
  "description": "Transfer tabs and tab groups between Chrome profiles",
  
  "permissions": [
    "tabs",
    "tabGroups"
  ],
  
  "host_permissions": [
    "http://localhost:7878/*"
  ],
  
  "action": {
    "default_popup": "popup.html"
  },
  
  "background": {
    "service_worker": "background.js"
  }
}